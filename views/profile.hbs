<!-- Nav Bar Menu Items -->
{{> navbar}}

<form style="border:1px solid #ccc padding:10px">
  <div class="container">
    <h1>PROFILE</h1>
    <p>Your Ultimate Sneaker Collection</p>
    <hr>
    <div id="index-gallery">
      <div class="item">
        <div id="collectionSneakers">
          <div id="collectionSneaker" class="col-md-55">
            <div class="collectionThumbnail">
              <div class="collectionSneakerInfo">
                <h2 class="collectionSneakerName"></h2>
                <p class="collectionBrand"></p>
                <p class="collectionShoeName"></p>
                <img alt="collectionSneaker" />
              </div>
            </div>
          </div>
        </div>
        <img src="/images/yeezy-black-red.png" alt="yeezy-black-red" />
        <p>YOUR COLLECTION</p>
      </div>
    </div>
    <hr>
  </div>
  <div class="container">
    <div id="index-gallery">
      <div class="item">
        <div id="wishlistSneakers">
          <div id="wishlistSneaker" class="col-md-55">
            <div class="wishlistThumbnail">
              <div class="wishlistSneakerInfo">
                <h2 class="wishlistSneakerName"></h2>
                <p class="wishlistBrand"></p>
                <p class="wishlistShoeName"></p>
                <img alt="wishlistsneaker" />
              </div>
            </div>
          </div>
        </div>
        <img src="/images/jordans.png" alt="jordans" />
        <p>YOUR WISH LIST</p>
      </div>
    </div>
  </div>
</form>
<br>
<br>
<br>
<br>

</body>

